        "ADULT_AGE : int = 18 # U.S. age\n",
        "\n",
        "def check_adult(age: int):\n",
        "    if age >= ADULT_AGE:\n",
        "        return True\n",
        "    else:\n",
        "        return False\n",
        "\n",
        "def main():\n",
        "  age : str = int(input(\"How old the person age is: \"))\n",
        "  print(check_adult(age))\n",
        "\n",
        "main()"
